{
  "name": "blinktrade",
  "version": "0.0.1",
  "description": "BlinkTrade Client for node.js",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "clean": "",
    "lint": "eslint src test",
    "flow": "flow check",
    "check": "npm run lint && npm run test ",
    "build:src": "babel src --out-dir lib",
    "build:dev": "cross-env NODE_ENV=development webpack ./src/index.js dist/blinktrade.js",
    "build:prod": "cross-env NODE_ENV=production webpack ./src/index.js dist/blinktrade.min.js",
    "build": "npm run build:src && npm run build:dev && npm run build:prod",
    "test": "NODE_ENV=test mocha --compilers js:babel-register --recursive",
    "prepublish": "npm run build"
  },
  "keywords": [
    "blinktrade",
    "node"
  ],
  "author": "Cesar Augusto, Rodrigo Souza",
  "license": "GPLv3",
  "dependencies": {
    "eventemitter2": "^2.1.0",
    "fetch-jsonp": "^1.0.1",
    "fingerprintjs2": "^1.4.1",
    "ip": "^1.1.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "macaddress": "^0.2.8",
    "nodeify": "^1.0.0",
    "sjcl": "^1.0.3",
    "ws": "^1.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^2.0.1",
    "eslint": "2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint_d": "^4.0.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.2",
    "moment": "^2.14.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.5",
    "webpack": "^1.13.2",
    "webpack-node-externals": "^1.3.3"
  }
}
